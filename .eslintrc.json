{
  "env": {
      "browser": true,
      "node": true,
      "mocha": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
     "strict": 0,
      "semi": 2
  }
}
